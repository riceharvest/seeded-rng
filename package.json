{
  "name": "@opensourceframework/seeded-rng",
  "version": "0.1.1",
  "description": "Seeded random number generator for reproducible randomness - NOT cryptographically secure",
  "keywords": [
    "random",
    "rng",
    "seeded",
    "deterministic",
    "reproducible",
    "game",
    "simulation",
    "lcg"
  ],
  "author": "OpenSource Framework Contributors",
  "contributors": [
    {
      "name": "Rice Harvest",
      "url": "https://github.com/riceharvest"
    }
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist coverage node_modules"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riceharvest/seeded-rng.git"
  },
  "bugs": {
    "url": "https://github.com/riceharvest/seeded-rng/issues"
  },
  "homepage": "https://github.com/riceharvest/seeded-rng#readme",
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "GitHub",
    "url": "https://github.com/sponsors/riceharvest"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
